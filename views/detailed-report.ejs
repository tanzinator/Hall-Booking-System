<html>

<head>




  <!-- <script type="text/javascript" charset="utf8" src="/jsdt/jquerymin182.js"></script> -->
  <link rel="stylesheet" type="text/css"
    href="https://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.9.0/css/jquery.dataTables.css">
  <link rel="stylesheet" type="text/css"
    href="https://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.9.0/css/jquery.dataTables_themeroller.css">
  <script type="text/javascript" charset="utf8"
    src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.7.1.min.js"></script>
  <script type="text/javascript" charset="utf8"
    src="https://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.9.0/jquery.dataTables.min.js"></script>

  <script>
    $(document).ready(function () {
      $('#receiptTable').DataTable();
    });
  </script>
</head>
<title>Detailed Report for <%=fromDate%> to <%=toDate%></title>

<%if(receipts.length===0){%>
<p>
  Detailed Report for <%=fromDate%> to <%=toDate%>
</p>
<h2>No Receipts Recorded For That Date Range <a href="/reports/detailed">Go Back</a></h2>
<%} else { %>
<p>
  Detailed Report for <%=fromDate%> to <%=toDate%>
</p>
<table id="receiptTable" name="receiptTable">
  <thead>
    <tr>
      <th>
        Receipt ID
      </th>
      <th>
        Date
      </th>
      <th>
        Services

      </th>
      <th>
        Total Amount
      </th>





    </tr>
  </thead>
  <tbody>


    <%receipts.forEach(curr=>{%>
    <tr>
      <td>
        <%=curr.receipt_id%>
      </td>
      <td>
        <%=curr.ISTCreatedAt%>
      </td>
      <td>
        <%=curr.services%>
      </td>
      <td>
        <%=curr.total_amount%>
      </td>



    </tr>



    <%});}%>
  </tbody>
</table>

<h2><a href="/receipt/detailed-report">Go Back</a></h2>
<h2><a href="/receipt/summary-report">Summary Report</a></h2>
<h2><a href="/receipt/reports">Reports Home</a></h2>
<h2><a href="/admin/home">Admin Home</a></h2>
<footer>
    <div style="text-align: center;">
